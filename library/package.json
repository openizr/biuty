{
  "name": "biuty",
  "version": "PACKAGE_VERSION",
  "type": "module",
  "main": "./main.js",
  "types": "./main.d.ts",
  "description": "SASS and components library, without the superfluous",
  "repository": "https://github.com/openizr/biuty",
  "bugs": "https://github.com/openizr/biuty/issues",
  "keywords": [
    "ui",
    "light",
    "typescript",
    "design",
    "system",
    "components",
    "react",
    "svelte",
    "vue",
    "sass",
    "styling",
    "themes",
    "library"
  ],
  "contributors": [
    {
      "name": "Matthieu Jabbour",
      "email": "matthieu.jabbour@gmail.com",
      "homepage": "https://github.com/matthieujabbour"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "@testing-library/react": "^13.3.0",
    "@testing-library/svelte": "^3.1.3",
    "@testing-library/vue": "^6.6.1",
    "@types/prop-types": "^15.7.4",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "aria-query": "^5",
    "coveralls": "^3.1.1",
    "node-sass": "^7.0.1",
    "typescript-dev-kit": "^5.1.1"
  },
  "eslintConfig": {
    "extends": [
      "./node_modules/typescript-dev-kit/main.js"
    ],
    "overrides": [
      {
        "files": [
          "*.d.ts"
        ],
        "rules": {
          "import/no-unresolved": "off",
          "import/no-extraneous-dependencies": "off",
          "import/export": "off",
          "import/no-relative-packages": "off",
          "import/no-cycle": "off",
          "import/no-self-import": "off",
          "max-classes-per-file": "off"
        }
      },
      {
        "files": [
          "*.tsx",
          "*.vue"
        ],
        "rules": {
          "vue/no-v-html": "off",
          "react/no-danger": "off",
          "jsx-a11y/no-autofocus": "off",
          "jsx-a11y/label-has-associated-control": "off",
          "jsx-a11y/no-noninteractive-element-interactions": "off"
        }
      }
    ]
  },
  "dependencies": {},
  "peerDependencies": {
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "svelte": "^3.49.0",
    "vue": "^3.2.37"
  },
  "peerDependenciesMeta": {
    "prop-types": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "svelte": {
      "optional": true
    },
    "vue": {
      "optional": true
    }
  },
  "engines": {
    "node": ">= 14.0.0",
    "npm": ">= 7.0.0"
  },
  "tsDevKitConfig": {
    "target": "node",
    "entries": {
      "main": "./scripts/main.ts",
      "react": "./scripts/react.ts",
      "vue": "./scripts/vue.ts",
      "svelte": "./scripts/svelte.ts"
    },
    "runInDev": false,
    "srcPath": "src",
    "distPath": "dist",
    "banner": "/*!\n * Copyright (c) Openizr. All Rights Reserved.\n *\n * This source code is licensed under the MIT license found in the\n * LICENSE file in the root directory of this source tree.\n */",
    "env": {
      "development": {},
      "production": {}
    }
  },
  "scripts": {
    "init": "node_modules/typescript-dev-kit/scripts/init.sh",
    "dev": "cd node_modules/typescript-dev-kit/ && node scripts/dev",
    "dev:sass": "node-sass -w --output-style=expanded src/styles/index.scss -o dist/css/",
    "build": "cd node_modules/typescript-dev-kit/ && node scripts/build && cd ../../ && cp src/scripts/*.d.ts dist && rm dist/index.d.ts && cp -r src/styles/* dist",
    "test": "cd node_modules/typescript-dev-kit/ && node scripts/test",
    "check": "cd node_modules/typescript-dev-kit/ && node scripts/check",
    "postinstall": "rm -f node_modules/.eslintcache && test -n \"$POSTINSTALLED\" || POSTINSTALLED=1 yarn add --peer prop-types react react-dom vue svelte"
  }
}