{
  "devDependencies": {
    "@types/prop-types": "^15.7.5",
    "@types/react": "^18.0.11",
    "@types/react-dom": "^18.0.5",
    "typescript-dev-kit": "^5.1.1"
  },
  "eslintConfig": {
    "extends": [
      "./node_modules/typescript-dev-kit/main.js"
    ],
    "overrides": [
      {
        "files": [
          "*.tsx",
          "*.vue",
          "*.svelte"
        ],
        "rules": {
          "no-console": "off",
          "react/static-property-placement": "off"
        }
      }
    ]
  },
  "dependencies": {
    "basx": "^2.0.1",
    "biuty": "file:../../../library/dist",
    "diox": "^5.0.2",
    "prop-types": "^15.7.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "svelte": "^3.48.0",
    "vue": "^3.2.36"
  },
  "tsDevKitConfig": {
    "target": "web",
    "html": "./html/index.html",
    "devServer": {
      "host": "0.0.0.0",
      "port": 5040
    },
    "splitChunks": false,
    "srcPath": "src",
    "distPath": "public",
    "banner": "",
    "env": {
      "development": {
        "ENV": "development",
        "NODE_ENV": "development"
      },
      "production": {
        "ENV": "production",
        "NODE_ENV": "production"
      }
    }
  },
  "scripts": {
    "init": "node_modules/typescript-dev-kit/scripts/init.sh",
    "dev": "cd node_modules/typescript-dev-kit/ && node scripts/dev",
    "build": "cd node_modules/typescript-dev-kit/ && node scripts/build",
    "test": "cd node_modules/typescript-dev-kit/ && node scripts/test",
    "check": "cd node_modules/typescript-dev-kit/ && node scripts/check",
    "doc": "node_modules/.bin/typedoc --out ./doc/ --exclude \"**/*.js\" --exclude \"**/__+(tests|mocks)__/**\" src/scripts/react.tsx",
    "postinstall": "rm -f node_modules/.eslintcache"
  }
}